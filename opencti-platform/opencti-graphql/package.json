{
  "name": "opencti-graphql",
  "version": "2.1.1",
  "main": "src/server.js",
  "author": "Luatix",
  "license": "Apache-2.0",
  "scripts": {
    "start": "babel-node --max_old_space_size=8192 -r node_modules/dotenv/config ./src/server.js --e development",
    "index": "babel-node -r node_modules/dotenv/config ./src/indexer.js",
    "build": "babel --out-dir ./dist ./src",
    "serv": "node dist/server.js",
    "migrate:add": "migrate create --migrations-dir=./src/migrations",
    "lint": "eslint .",
    "test": "yarn build && jest --coverage --e test",
    "clean": "babel-node ./src/database/clean.js"
  },
  "bin": {
    "opencti-graphql": "./dist/server.js"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "config/**/*",
      "public/**/*"
    ]
  },
  "engines": {
    "node": ">= 12.* < 13.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/runtime": "^7.6.3",
    "@elastic/elasticsearch": "^7.4.0",
    "@godaddy/terminus": "^4.2.0",
    "amqplib": "^0.5.5",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.9.6",
    "await": "^0.2.6",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.1",
    "body-parser": "^1.19.0",
    "bytes": "^3.1.0",
    "compression": "^1.7.4",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "grakn-client": "^1.5.6",
    "graphql": "^14.5.8",
    "graphql-constraint-directive": "^1.4.1",
    "graphql-import": "^0.7.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-redis-subscriptions": "^2.1.1",
    "graphql-relay": "^0.6.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^4.0.5",
    "helmet": "^3.21.1",
    "ioredis": "^4.14.1",
    "iterall": "^1.2.2",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.0",
    "migrate": "^1.6.2",
    "mime-types": "^2.1.24",
    "minio": "^7.0.12",
    "moment": "^2.24.0",
    "nconf": "^0.10.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "python-shell": "^1.0.8",
    "ramda": "^0.26.1",
    "rxjs": "^6.5.3",
    "uuid": "^3.3.3",
    "validator": "^11.1.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "apollo-server-testing": "^2.9.6",
    "babel-jest": "^25.0.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^25.0.0",
    "pkg": "^4.4.0",
    "prettier": "^1.18.2"
  },
  "jest": {
    "testRegex": "/tests/.*/.*-test.js$"
  },
  "directories": {
    "test": "tests"
  },
  "keywords": [],
  "description": ""
}
